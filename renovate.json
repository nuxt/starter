{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "baseBranches": [
    "v3",
    "v3-codesandbox",
    "v3-stackblitz",
    "v3-vercel",
    "module",
    "doc-driven",
    "content",
    "v2",
    "v2-bridge",
    "v2-stackblitz",
    "v2-codesandbox"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "@types/node"
      ],
      "rangeStrategy": "auto"
    }
  ]
}
